@model MvcCv.Models.Entity.TBLSOCIALMEDIA

@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Social Media Edit</h4>

<br />
<form method="post">
    @Html.Label("Social Media Name")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İcon")
    @Html.TextBoxFor(x => x.İcon, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Update</button>
</form>

